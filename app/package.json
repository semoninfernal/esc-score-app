{
  "name": "esc-score-app",
  "version": "0.0.1",
  "description": "Voting app for esc",
  "private": true,
  "main": "bin/server.js",
  "keywords": [
    "react"
  ],
  "author": "Simon Far√•s <simon.faras@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "build": "node ./node_modules/better-npm-run build",
    "lint": "eslint -c .eslintrc src api",
    "build-dev": "node ./node_modules/better-npm-run build-dev",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "dev-tdd": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run test-tdd\"",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "test": "node ./node_modules/better-npm-run test",
    "test-tdd": "node ./node_modules/better-npm-run test-tdd"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 80,
        "API_PORT": 88,
        "API_HOST": "api"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "API_PORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    },
    "test": {
      "command": "mocha",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "test-tdd": {
      "command": "mocha --watch",
      "env": {
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "6.6.1",
    "babel-register": "6.6.0",
    "better-npm-run": "0.0.3",
    "cookie-parser": "1.4.1",
    "express": "4.13.3",
    "history": "2.0.1",
    "hoist-non-react-statics": "1.0.3",
    "http-proxy": "1.12.0",
    "install": "0.4.1",
    "lodash.curry": "^4.0.1",
    "lodash.debounce": "^4.0.6",
    "lodash.groupby": "^4.3.0",
    "lodash.orderby": "^4.2.1",
    "lodash.sumby": "^4.4.0",
    "npm": "3.5.2",
    "piping": "^0.3.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-flip-move": "2.2.0",
    "react-redux": "4.0.0",
    "react-router": "2.2.4",
    "react-router-redux": "4.0.0",
    "redux": "3.0.4",
    "redux-async-connect": "1.0.0-rc4",
    "redux-form": "5.0.1",
    "reselect": "2.0.1",
    "scroll-behavior": "^0.3.3",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.4.0",
    "webpack-isomorphic-tools": "2.2.14"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.6.4",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.5",
    "concurrently": "^0.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-import": "^0.9.1",
    "eslint-plugin-react": "^3.8.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "faker": "^3.0.1",
    "file-loader": "^0.8.5",
    "json-server": "^0.8.2",
    "mocha": "^2.3.3",
    "mocha-silent-reporter": "^1.0.0",
    "node-sass": "^3.4.2",
    "qs": "^6.1.0",
    "react-addons-create-fragment": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^3.1.2",
    "skin-deep": "^0.13.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  },
  "engines": {
    "node": "5.0.0"
  }
}
